class Exercico4 {
   String nome;
   String departamento;
   String rg;
   double salario;
   
   void recebeAumento(double valor){
      this.salario += valor;
   }
   
   double ganhoAnual(){
      double ganhoAnual = this.salario * 12;
      return ganhoAnual;
   }

   void mostra(){
      System.out.println(
         "Nome: " + this.nome +
         "\n Departamento: " + this.departamento +
         "\n Rg: " + this.rg +
         "\n Salario: " + this.salario +
         "\n Ganho Anual: " + this.ganhoAnual());
   }
 }
        
